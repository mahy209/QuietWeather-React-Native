{
  "expo": {
    "name": "Quiet weather",
    "slug": "Quiet_weather",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/weathericon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/weathericon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "in.mahymohab.QuietWeather",
      "versionCode": 1,
      "permissions": ["LOCATION"]
    },
    "web": {
      "favicon": "./assets/weathericon.png"
    }
  }
}
